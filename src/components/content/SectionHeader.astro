---
interface Props {
  command: string;
  description: string;
}

const { command, description } = Astro.props;
---

<div class="flex flex-col gap-2">
  <h2 class="text-lg font-semibold text-green-primary font-mono md:text-[28px]">{command}</h2>
  <p class="text-[13px] text-text-tertiary font-mono">{description}</p>
</div>
